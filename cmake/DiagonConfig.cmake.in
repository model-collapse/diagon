@PACKAGE_INIT@

# Diagon CMake Package Configuration

include(CMakeFindDependencyMacro)

# Find dependencies
find_dependency(ZLIB REQUIRED)
find_dependency(zstd REQUIRED)
find_dependency(lz4 REQUIRED)

# Include targets
include("${CMAKE_CURRENT_LIST_DIR}/DiagonTargets.cmake")

# Set variables for consumers
set(DIAGON_VERSION "@PROJECT_VERSION@")
set(DIAGON_INCLUDE_DIRS "@CMAKE_INSTALL_FULL_INCLUDEDIR@/diagon")
set(DIAGON_LIBRARIES Diagon::diagon_core Diagon::diagon_columns Diagon::diagon_compression Diagon::diagon_simd)

check_required_components(Diagon)
